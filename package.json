{
  "name": "admin-panel-template-reactjs",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/user-event": "^12.1.6",
    "@types/yup": "^0.29.8",
    "axios": "^0.20.0",
    "eslint-plugin-filenames": "^1.3.2",
    "formik": "^2.2.0",
    "formik-material-ui": "^3.0.0",
    "history": "^5.0.0",
    "lodash": "^4.17.20",
    "obj-str": "^1.0.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "yup": "^0.29.3"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint"
    ],
    "src/**/*.{json,css,scss,sass}": [
      "yarn format"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "npm": "please use yarn",
    "yarn": ">= 1.19.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "start:stg": "env-cmd -f .env.stg react-app-rewired start",
    "start:prod": "env-cmd -f .env.prod react-app-rewired start",
    "build": "react-app-rewired build",
    "build:stg": "env-cmd -f .env.stg react-app-rewired build",
    "build:prod": "env-cmd -f .env.prod react-app-rewired build",
    "test": "react-app-rewired test",
    "test:stg": "env-cmd -f .env.stg react-app-rewired test",
    "test:prod": "env-cmd -f .env.prod react-app-rewired test",
    "eject": "react-scripts eject",
    "format": "prettier src/**/*.{ts,tsx,json} --write",
    "lint": "eslint src/**/*.{ts,tsx} --fix --quiet"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@types/obj-str": "^1.0.0",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/redux-persist": "^4.3.1",
    "customize-cra": "^1.0.0",
    "customize-cra-react-refresh": "^1.1.0",
    "env-cmd": "^10.1.0",
    "eslint-config-airbnb-typescript-prettier": "^3.1.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "react-app-rewired": "^2.1.6",
    "typescript": "~3.7.2"
  }
}
